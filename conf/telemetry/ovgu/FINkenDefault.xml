<?xml version="1.0"?>
<!DOCTYPE telemetry SYSTEM "../telemetry.dtd">

<telemetry>

    <!--Needed for SD Card Logger-->
    <process name="Logger">
        <mode name="default">
            <message name="ENERGY" period="0.005"/>
            <message name="ATTITUDE" period="0.005"/>
            <message name="ROTORCRAFT_FP" period="0.005"/>
            <message name="INS" period="0.005"/>
            <message name="IMU_GYRO_SCALED" period="0.005"/>
            <message name="IMU_ACCEL_SCALED" period="0.005"/>
            <message name="IMU_MAG_SCALED" period="0.005"/>
            <message name="ROTORCRAFT_NAV_STATUS" period="0.1"/>
            <message name="ROTORCRAFT_STATUS" period="0.1"/>
            <message name="VIRT_BARO" period="0.005"/>
            <message name="GPS_INT" period=".01"/>
        </mode>
    </process>

    <process name="Main">

        <mode name="default" key_press="d">
            <message name="ENERGY" period="0.05"/>
            <message name="ROTORCRAFT_FP" period="1"/>
            <message name="ROTORCRAFT_NAV_STATUS" period="0.1"/>
            <message name="ROTORCRAFT_STATUS" period="0.1"/>
            <message name="AUTOPILOT_VERSION" period="11.1"/>
            <message name="DL_VALUE" period="0.5"/>
            <message name="RC" period="1.0"/>
            <message name="ALIVE" period="2.1"/>
            <message name="DATALINK_REPORT" period="5.1"/>
            <message name="GPS" period="1.0"/>
            <message name="GPS_INT" period="1.0"/>
        </mode>

        <!--Needed for sd card logging -->
        <mode name="empty">
        </mode>


    </process>

</telemetry>

